<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Group?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="aPane" prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo.postgame.RankSceneController">
    <children>
        <StackPane AnchorPane.bottomAnchor="6.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <children>
                <Group StackPane.alignment="CENTER">
                    <children>
                        <Label fx:id="titleLB" layoutX="152.0" layoutY="56.0" text="TOP 10 Leaderboard">
                            <font>
                                <Font name="Calisto MT Bold" size="38.0" />
                            </font>
                        </Label>
                        <Label fx:id="winnersScoreTF" layoutX="396.0" layoutY="111.0" prefHeight="314.0" prefWidth="83.0">
                            <font>
                                <Font name="Monospaced Regular" size="19.0" />
                            </font>
                        </Label>
                        <Label fx:id="winnersNameTF" layoutX="184.0" layoutY="111.0" prefHeight="314.0" prefWidth="204.0">
                            <font>
                                <Font name="Monospaced Regular" size="19.0" />
                            </font></Label>
                        <Button fx:id="quitBtn1" layoutX="210.0" layoutY="512.0" mnemonicParsing="false" onAction="#quitBtnOnClick" prefHeight="42.0" prefWidth="239.0" style="-fx-background-color: #f3f2f2; -fx-border-style: solid; -fx-border-color: #c1bdbd; -fx-background-radius: 10; -fx-border-radius: 10;" text="QUIT">
                     <font>
                        <Font name="Monospaced Bold" size="16.0" />
                     </font></Button>
                  <Button fx:id="backToMenuBtn" layoutX="210.0" layoutY="452.0" mnemonicParsing="false" onAction="#backToMenuBtnOnClick" prefHeight="42.0" prefWidth="239.0" style="-fx-background-color: #f3f2f2; -fx-border-style: solid; -fx-border-color: #c1bdbd; -fx-background-radius: 10; -fx-border-radius: 10;" text="BACK TO MENU">
                     <font>
                        <Font name="Monospaced Bold" size="16.0" />
                     </font>
                  </Button>
                    </children>
                </Group>
            </children>
        </StackPane>
    </children>
</AnchorPane>
